package d01;

public class LTV implements TV {

	@Override
	public void powerOn() {
		System.out.println("LTV powerON");
	}

	@Override
	public void powerOff() {
		System.out.println("LTV powerOff");
	}

	@Override
	public void powerUp() {
		System.out.println("LTV volumeUp");
	}

	@Override
	public void powerDown() {
		System.out.println("LTV volumeDown");
	}

}
